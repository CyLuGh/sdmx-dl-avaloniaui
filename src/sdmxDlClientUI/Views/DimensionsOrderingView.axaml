<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="using:sdmxDlClient.ViewModels"
             xmlns:rxui="using:Avalonia.ReactiveUI"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="sdmxDlClientUI.Views.DimensionsOrderingView">
  <TabControl>
    <TabItem Header="Browser">
      <TreeView Margin="5" />
    </TabItem>
    <TabItem Header="Dimensions">
      <ScrollViewer>
        <ItemsControl Name="ItemsControlDimensions">
          <ItemsControl.DataTemplates>
            <DataTemplate DataType="{x:Type viewModels:DimensionViewModel}">
              <rxui:ViewModelViewHost ViewModel="{Binding}" />
            </DataTemplate>
          </ItemsControl.DataTemplates>
        </ItemsControl>
      </ScrollViewer>
    </TabItem>
  </TabControl>
</UserControl>