      </Grid.RowDefinitions>
      <!--<scottPlot:AvaPlot Grid.Row="1" />-->
      <ComboBox Name="ComboBoxTimeSeries"
                HorizontalAlignment="Stretch"
                Grid.Row="0">

        <!--<ComboBox.DataTemplates>
          <DataTemplate DataType="{x:Type viewmodels:TimeSerieDisplayViewModel}">
            <TextBlock Text="{Binding Path=Header}" />
          </DataTemplate>
        </ComboBox.DataTemplates>-->
