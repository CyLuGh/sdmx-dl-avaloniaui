      <TextBlock Name="TextTest" Grid.Row="1" />
      <TextBlock Name="TextTest2" Grid.Row="2" />
      <!--<lvc:CartesianChart Name="Chart" Grid.Row="1" />-->
    </Grid>
  </Border>
</UserControl>